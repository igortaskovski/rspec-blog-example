<%= form_tag url: login_path do %>

  <p>
    <%= label_tag 'Email' %><br>
    <%= email_field_tag :email %>
  </p>
 
  <p>
    <%= label_tag 'Password' %><br>
    <%= password_field_tag :password %>
  </p>

  <p>
    <%= submit_tag 'Log In' %>
  </p>

<% end %>

<%= link_to 'Signup', new_user_path %>